  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.5.0",
    "inquirer": "^8.2.6",
    "simple-git": "^3.19.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
