{
  "name": "snail-git-add",
  "version": "1.0.0",
  "description": "Interactive git add, commit and push tool with conventional commits and remote checks",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "snail-git-add": "bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
